{
  "paths": {
    "train_batches": "./data/java-small/training_preprocessed",
    "validation_batches": "./data/java-small/validation_preprocessed",
    "test_batches": "./data/java-small/test_preprocessed",

    "vocabulary_path": "./data/java-small/vocabulary.pkl",
    "labels_path": "./data/java-small/labels.pkl"
  },

  "embedding": {
    "name": "SubTokenEmbedding",
    "params": {
      "out_size": 128
    }
  },

  "encoder": {
    "name": "TreeLSTM",
    "params": {
      "x_size": 128,
      "h_size": 128
    }
  },

  "decoder": {
    "name": "LSTMDecoder",
    "params": {
      "embedding_size": 128,
      "hidden_size": 128
    }
  },

  "batch_size": 8,
  "n_epochs": 10,
  "lr": 0.01,
  "weight_decay": 1e-4,
  "clip_norm": 5,

  "verbosity_step": 10
}
